    Name = "Glizzy Hub",
    LoadingTitle = "Glizzy Hub (Private)",
    LoadingSubtitle = "by Antwan/Juan",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil,
       FileName = "Glizzy"
    },
    Discord = {
       Enabled = false,
       Invite = "noinvitelink",
       RememberJoins = true
    },
    KeySystem = true,
    KeySettings = {
       Title = "Glizzy Hub Key System",
       Subtitle = "Key System",
       Note = "No method of obtaining the key is provided",
